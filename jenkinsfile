pipeline {
  agent {
      docker { image 'ubuntu:latest' }
  }
  stages {
    node {
      def remote = [:]
      remote.name = 'test'
      remote.host = 'test.domain.com'
      remote.user = 'root'
      remote.password = 'password'
      remote.allowAnyHosts = true
        stage('Remote SSH') {
          sshRemove remote: remote, path: "abc.sh"
        }
    }
      
  }
}