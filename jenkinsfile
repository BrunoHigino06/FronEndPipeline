pipeline {
  agent any
      stage('Remote SSH') {
          def remote = [:]
          remote.name = 'test'
          remote.host = '172.17.0.4'
          remote.user = 'root'
          remote.password = '040188'
          remote.allowAnyHosts = true
          sshCommand remote: remote, command: "ls -lrt"
          sshCommand remote: remote, command: "for i in {1..5}; do echo -n \"Loop \$i \"; date ; sleep 1; done"
        }
}
